{
  "name": "gobuhat-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ts.build": "webpack-cli --config webpack.conf.js",
    "ts.watch": "webpack-cli --config webpack.conf.js --watch",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "start.dev": "nodemon -w . ./bin/www",
    "start.prod": "node ./bin/www",
    "test.watch": "nodemon -w . --exec 'jest tests/* --runInBand || true'",
    "test": "jest tests/* -b -i --forceExit"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cookie-parser": "~1.4.3",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.2",
    "morgan": "~1.9.0",
    "tslint": "^5.10.0",
    "webpack-cli": "^2.1.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/crypto-js": "^3.1.39",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/http-errors": "^1.6.1",
    "@types/lodash": "^4.14.109",
    "@types/mongoose": "^5.0.14",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.1.2",
    "jest": "^22.4.4",
    "nodemon": "^1.17.5",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3"
  }
}
